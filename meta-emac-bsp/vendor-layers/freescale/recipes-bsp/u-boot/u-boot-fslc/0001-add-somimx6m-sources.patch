+        else if (board_type == BOARD_IS_DUAL)
+                puts("EMAC SOM-iMX6 Dual\n");
+        else if (board_type == BOARD_IS_SOLO)
+                puts("EMAC SOM-iMX6 Solo\n");
+        else
+                printf("IMX6 - cputype : %02x\n", cpu_type(get_cpu_rev()));
+
+        return 0;
+}
+
+void reset_cpu(void){
+	// should really use imx_wdt here so we can set as extrenal or not.
+ 	gpio_request(WDOG_EN, "Watchdog enable");
+        gpio_direction_output(WDOG_EN, 0);
+}
