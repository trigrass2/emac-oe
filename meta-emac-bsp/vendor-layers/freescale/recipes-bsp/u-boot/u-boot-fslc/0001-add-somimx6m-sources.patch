+static void imx6_spl_dram_cfg_size(u32 ram_size)
+{
+	if (is_mx6dq()){
+		mx6dq_dram_iocfg(32, &mx6q_ddr_ioregs, &mx6q_grp_ioregs);
+	}
+	else{
+		mx6sdl_dram_iocfg(32, &mx6dl_ddr_ioregs, &mx6sdl_grp_ioregs);
+	}
+
+	if (is_cpu_type(MXC_CPU_MX6D)) {
+		mem_ddr.rowaddr = 15;
+		mx6_dram_cfg(&ddr_sysinfo, &mx6q_1g_mmcd_calib, &mem_ddr);
+	}
+	else if (is_cpu_type(MXC_CPU_MX6Q)) {
+		mem_ddr.rowaddr = 16;
+		mx6_dram_cfg(&ddr_sysinfo, &mx6q_2g_mmcd_calib, &mem_ddr);
+	}
+	else if (is_cpu_type(MXC_CPU_MX6SOLO)) {
+		mem_ddr.rowaddr = 14;
+		mx6_dram_cfg(&ddr_sysinfo, &mx6dl_512m_mmcd_calib, &mem_ddr);
+	}
