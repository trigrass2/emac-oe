+void getboardType(void){
+    u32 cputype = cpu_type(get_cpu_rev());
+    switch (cputype) {
+    case MXC_CPU_MX6SOLO:
+            board_type = BOARD_IS_SOLO;
+            break;
+    case MXC_CPU_MX6D:
+            setup_iomux_pcie();
+            board_type = BOARD_IS_DUAL;
+            break;
+    case MXC_CPU_MX6Q:
+            setup_iomux_pcie();
+            board_type = BOARD_IS_QUAD;
+            break;
+    }
+}
+
+void board_boot_order(u32 *spl_boot_list){
+        u32 bmode = imx6_src_get_boot_mode();
+        u8 boot_dev = BOOT_DEVICE_MMC2;
+
+        switch ((bmode & IMX6_BMODE_MASK) >> IMX6_BMODE_SHIFT) {
+        case IMX6_BMODE_SD:
+        case IMX6_BMODE_ESD:
+        case IMX6_BMODE_MMC:
+        case IMX6_BMODE_EMMC:
+                boot_dev = BOOT_DEVICE_MMC1;
+                break;
+        case IMX6_BMODE_SERIAL_ROM:
+                boot_dev = BOOT_DEVICE_SPI;
+                break;
+        case IMX6_BMODE_SATA:
+                boot_dev = BOOT_DEVICE_SATA;
+                break;
+        default:
+                printf("Wrong board boot order\n");
+                break;
+        }
+
+//        printf("Boot BOOTDEV  mode is %x ", bmode);
+//        printf("  BOOT DEV(FALLBACK) %x \n", boot_dev);
+        spl_boot_list[0] = BOOT_DEVICE_MMC1;
+        spl_boot_list[1] = BOOT_DEVICE_SPI;
+        spl_boot_list[2] = boot_dev;
+        spl_boot_list[3] = BOOT_DEVICE_BOARD;
+
+}
