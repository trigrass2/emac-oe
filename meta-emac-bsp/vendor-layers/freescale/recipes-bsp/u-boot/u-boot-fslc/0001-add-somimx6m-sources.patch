+void board_init_f(ulong dummy)
+{
+	arch_cpu_init();
+	ccgr_init();
+
+	/* setup GP timer */
+	timer_init();
+
+	/* setup uart */
+	setup_iomux_uart();
+
+	/* setup spi */
+	setup_iomux_spi();
+
+	/* UART clocks enabled and gd valid - init serial console */
+	preloader_console_init();
+
+	/* DDR initialization */
+	imx6_spl_dram_cfg();
+
+	/* Clear the BSS. */
+	memset(__bss_start, 0, __bss_end - __bss_start);
+
+	/* load/boot image from boot device */
+	board_init_r(NULL, 0);
+}
