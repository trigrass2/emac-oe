/dts-v1/;
#include "at91sam9x25.dtsi"

/ {
	model = "EMAC IPAC-9X25";
	compatible = "atmel,at91sam9x25ek", "atmel,at91sam9x5ek", "atmel,at91sam9x5", "atmel,at91sam9";

	chosen {
		bootargs = "root=/dev/mtdblock1 rw rootfstype=ubifs ubi.mtd=1 root=ubi0:rootfs";
		stdout-path = "serial0:115200n8";
		u-boot,dm-pre-reloc;
	};

	aliases {
		serial1 = &usart0;
		serial3 = &uart0;
		serial5 = &dbgu;
	};

	memory {
		reg = <0x20000000 0x8000000>;
	};

	clocks {
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;

		main_clock: clock@0 {
			compatible = "atmel,osc", "fixed-clock";
			clock-frequency = <12000000>;
		};

		slow_xtal {
			clock-frequency = <32768>;
		};

		main_xtal {
			clock-frequency = <12000000>;
		};
	};
	
	wdog: watchdog {
		/* MAX6747 */
		compatible = "linux,wdt-gpio";
		gpios = <&pioC 6 GPIO_ACTIVE_LOW>;
		hw_algo = "toggle";
		always-running;
		hw_margin_ms = <1000>;
	};

	ahb {
		apb {
			pinctrl@fffff400 {
				adc0 {
					pinctrl_adc0: adc0-board {
						atmel,pins =
							<AT91_PIOB 11 AT91_PERIPH_A AT91_PINCTRL_NONE
							 AT91_PIOB 12 AT91_PERIPH_A AT91_PINCTRL_NONE
							 AT91_PIOB 13 AT91_PERIPH_A AT91_PINCTRL_NONE
							 AT91_PIOB 14 AT91_PERIPH_A AT91_PINCTRL_NONE
							 AT91_PIOB 15 AT91_PERIPH_A AT91_PINCTRL_NONE
							 AT91_PIOB 16 AT91_PERIPH_A AT91_PINCTRL_NONE
							 AT91_PIOB 17 AT91_PERIPH_A AT91_PINCTRL_NONE>;
					};
				};

				mmc0 {
					pinctrl_board_mmc0: mmc0-board {
						atmel,pins =
							<AT91_PIOC 4 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP_DEGLITCH>;
					};
				};

				gpio {
					pinctrl_board_gpio: gpio0-board {
						atmel,pins =
							<AT91_PIOD 0 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 1 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 2 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 3 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 4 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 5 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 6 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 7 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 8 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 9 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 10 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 11 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 12 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 13 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 14 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 15 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 16 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 17 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 18 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 19 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 20 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOD 21 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP>;
					};
				};

				wdt_gpio {
					pinctrl_wdt: wdt-board {
						atmel,pins =
							<AT91_PIOC 6 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP
							 AT91_PIOC 7 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP>;
					};
				};

				led_gpio {
					pinctrl_led: led-board {
						atmel,pins =
							<AT91_PIOD 20 AT91_PERIPH_GPIO AT91_PINCTRL_NONE>;
					};
				};
			};

			adc0: adc@f804c000 {
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_adc0>;
				status = "okay";
				atmel,adc-channels-used = <0x7f>;
			};

			pwm0: pwm@f8034000 {
				pinctrl-names = "default";
				pinctrl-0 = <&pinctrl_pwm0_pwm0_1 &pinctrl_pwm0_pwm1_1>;
				compatible = "atmel,at91sam9rl-pwm";
				reg = <0xf8034000 0x400>;
				#pwm-cells = <3>;
				status = "okay";
			};

			ssc0: ssc@f0010000 {
				status = "okay";
			};

			mmc0: mmc@f0008000 {
				pinctrl-names = "default";
				pinctrl-0 = <
					&pinctrl_board_mmc0
					&pinctrl_mmc0_slot0_clk_cmd_dat0
					&pinctrl_mmc0_slot0_dat1_3>;
				status = "okay";
				slot@0 {
					reg = <0>;
					bus-width = <4>;
					/* cd-gpios = <&pioC 4 GPIO_ACTIVE_LOW>; */
				};
			};

			macb0: ethernet@f802c000 {
				phy-mode = "rmii";
				status = "okay";
			};

			macb1: ethernet@f8030000 {
				phy-mode = "rmii";
				status = "okay";
			};

			rtc: rtc@fffffeb0 {
				status = "okay";
			};

			dbgu: serial@fffff200 {
				status = "okay";
				serial {
					compatible = "atmel,at91rm9200-usart-serial";
				};
			};

			usart0: serial@f801c000 {
				pinctrl-names = "default";
				pinctrl-0 = <
					&pinctrl_usart0
					&pinctrl_usart0_rts
					&pinctrl_usart0_cts>;
				status = "okay";
				serial {
					compatible = "atmel,at91rm9200-usart-serial";
				};
			};

			usart3: serial@f8028000 {
				pinctrl-names = "default";
				pinctrl-0 = <
					&pinctrl_usart3
					&pinctrl_usart3_rts
					&pinctrl_usart3_cts>;
				status = "okay";
				serial {
					compatible = "atmel,at91rm9200-usart-serial";
				};
			};

			uart0: serial@f8040000 {
				status = "okay";
				serial {
					compatible = "atmel,at91rm9200-usart-serial";
				};
			};

			can1: can@f8004000 {
				status = "okay";
			};

			usb2: gadget@f803c000 {
				status = "okay";
			};

			i2c1: i2c@f8014000 {
				status = "okay";
			};
		};

		usb0: ohci@00600000 {
			status = "okay";
			num-ports = <3>;
			atmel,vbus-gpio = <0 /* &pioD 18 GPIO_ACTIVE_LOW *//* Activate to have access to port A */
					   &pioD 19 GPIO_ACTIVE_LOW
					   &pioD 20 GPIO_ACTIVE_LOW
					  >;
		};

		usb1: ehci@00700000 {
			status = "okay";
		};
	};

	gpio-leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_led>;
		led-stat {
			label = "status";
			gpios = <&pioD 20 1>;
			default-state = "on";
			linux,default-trigger = "none";
		};
	};

	gpios {
		compatible = "atmel,at91sam9x5-pinctrl", "atmel,at91rm9200-pinctrl", "simple-bus";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_board_gpio>;
	};
};
